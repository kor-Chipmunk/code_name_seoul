<div class="game">
  <div class="timer"><%= render partial: 'count', left_time: @left_time %></div>
  <img src="<%= @current_stage.s_img %>" width="100%">
  <%= form_tag '/script', id: 'form', remote: true, style: "position: absolute; bottom: -20px; right: 200px;" do %>
  <div class="btnContainer row">
    <%= submit_tag 'Next', id: "getBtn", class:"col-xs-offset-7" %>
  </div>
  <% end %>
</div>
